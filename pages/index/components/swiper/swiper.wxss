.swiper-container{
    height: 150px;
    width: 100%;
}

.item{
    height: 100%;
    line-height: 150px;
    text-align: center;
}

swiper-item:nth-child(1){
    background-color: darkmagenta;
}

swiper-item:nth-child(2){
    background-color:dimgray;
}

swiper-item:nth-child(3){
    background-color:dodgerblue;
}